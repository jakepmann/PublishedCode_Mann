BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(beta = BETA)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(se = SE)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(pval = P)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(chr = CHR)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(pos = POS)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(ref = REF)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(alt = ALT)
BBJ_ALT_MTARC1 <- BBJ_ALT_MTARC1 %>% rename(EAF = Frq)
BBJ_ALT_MTARC1 = subset(BBJ_ALT_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

BBJ_HbA1c_MTARC1 <- read.table(file = 'BBJ_HbA1c_MTARC1.tsv', sep = '\t', header = FALSE)
colnames(BBJ_HbA1c_MTARC1) <- BBJ_HbA1c_header
BBJ_HbA1c_MTARC1$trait <- "BBJ_HbA1c"
