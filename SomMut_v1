MAGIC_HbA1c_GPAM$EAF <- ""
MAGIC_HbA1c_GPAM = subset(MAGIC_HbA1c_GPAM, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

