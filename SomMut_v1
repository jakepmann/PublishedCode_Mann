## FinnGen summary stats

## FinnGen_ALCOLIVER 
FinnGen_ALCOLIVER_header <- read.delim(file = 'FinnGen_ALCOLIVER_header.tsv', sep = '\t', header = FALSE)


FinnGen_ALCOLIVER_ACVR2A <- read.table(file = 'FinnGen_ALCOLIVER_ACVR2A.tsv', sep = '\t', header = FALSE)
colnames(FinnGen_ALCOLIVER_ACVR2A) <- FinnGen_ALCOLIVER_header
FinnGen_ALCOLIVER_ACVR2A$trait <- "FinnGen_ALCOLIVER"
FinnGen_ALCOLIVER_ACVR2A$HCCgene <- "ACVR2A"
FinnGen_ALCOLIVER_ACVR2A = subset(FinnGen_ALCOLIVER_ACVR2A, beta !="")
FinnGen_ALCOLIVER_ACVR2A <- FinnGen_ALCOLIVER_ACVR2A %>% filter(between(pos, 147844517, 147930826))
FinnGen_ALCOLIVER_ACVR2A <- FinnGen_ALCOLIVER_ACVR2A %>% rename(se = sebeta)
FinnGen_ALCOLIVER_ACVR2A <- FinnGen_ALCOLIVER_ACVR2A %>% rename(pval = pval)
names(FinnGen_ALCOLIVER_ACVR2A)[names(FinnGen_ALCOLIVER_ACVR2A) == '#chrom'] <- 'chr'
FinnGen_ALCOLIVER_ACVR2A <- FinnGen_ALCOLIVER_ACVR2A %>% rename(EAF = maf)
FinnGen_ALCOLIVER_ACVR2A = subset(FinnGen_ALCOLIVER_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

