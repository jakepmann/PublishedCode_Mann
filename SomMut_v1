Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(pval = P)
Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(chr = Chr)
Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(pos = Pos)
Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(ref = NEA)
Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(alt = EA)
Sprack_T2DM_MTARC1 <- Sprack_T2DM_MTARC1 %>% rename(EAF = EAF)
Sprack_T2DM_MTARC1 = subset(Sprack_T2DM_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))


ALT_metan_MTARC1 <- read.table(file = 'ALT_metan_MTARC1.tsv', sep = '\t', header = FALSE)
colnames(ALT_metan_MTARC1) <- ALT_metan_header
ALT_metan_MTARC1$trait <- "ALT_metan"
ALT_metan_MTARC1$HCCgene <- "MTARC1"
ALT_metan_MTARC1 = subset(ALT_metan_MTARC1, beta !="")
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(beta = beta)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(se = standard_error)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(pval = p_value)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(chr = chromosome)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(pos = base_pair_location)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(ref = other_allele)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(alt = effect_allele)
ALT_metan_MTARC1 <- ALT_metan_MTARC1 %>% rename(EAF = effect_allele_frequency)
ALT_metan_MTARC1 = subset(ALT_metan_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

BBJ_ALT_MTARC1 <- read.table(file = 'BBJ_ALT_MTARC1.tsv', sep = '\t', header = FALSE)
colnames(BBJ_ALT_MTARC1) <- BBJ_ALT_header
BBJ_ALT_MTARC1$trait <- "BBJ_ALT"
BBJ_ALT_MTARC1$HCCgene <- "MTARC1"
