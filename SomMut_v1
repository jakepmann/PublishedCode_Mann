colnames(BBJ_HbA1c_HSD17B13) <- BBJ_HbA1c_header
BBJ_HbA1c_HSD17B13$trait <- "BBJ_HbA1c"
BBJ_HbA1c_HSD17B13$HCCgene <- "HSD17B13"
