GPAM_pLoF_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000119927_GPAM_pLoF.csv")
GPAM_mis_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000119927_GPAM_mis.csv")
GPAM_synon_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000119927_GPAM_synon.csv")

TNRC6B_pLoF_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000100354_TNRC6B_pLoF.csv")
TNRC6B_mis_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000100354_TNRC6B_mis.csv")
TNRC6B_synon_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000100354_TNRC6B_synon.csv")

ALB_pLoF_genebass$gene <- "ALB"
ALB_pLoF_genebass$burden <- "pLoF"
ALB_mis_genebass$gene <- "ALB"
ALB_mis_genebass$burden <- "mis"
ALB_synon_genebass$gene <- "ALB"
ALB_synon_genebass$burden <- "synon"
ACVR2A_pLoF_genebass$gene <- "ACVR2A"
ACVR2A_pLoF_genebass$burden <- "pLoF"
ACVR2A_mis_genebass$gene <- "ACVR2A"
ACVR2A_mis_genebass$burden <- "mis"
ACVR2A_synon_genebass$gene <- "ACVR2A"
ACVR2A_synon_genebass$burden <- "synon"
CIDEB_pLoF_genebass$gene <- "CIDEB"
CIDEB_pLoF_genebass$burden <- "pLoF"
CIDEB_mis_genebass$gene <- "CIDEB"
CIDEB_mis_genebass$burden <- "mis"
CIDEB_synon_genebass$gene <- "CIDEB"
CIDEB_synon_genebass$burden <- "synon"
FOXO1_pLoF_genebass$gene <- "FOXO1"
FOXO1_pLoF_genebass$burden <- "pLoF"
FOXO1_mis_genebass$gene <- "FOXO1"
FOXO1_mis_genebass$burden <- "mis"
FOXO1_synon_genebass$gene <- "FOXO1"
FOXO1_synon_genebass$burden <- "synon"
GPAM_pLoF_genebass$gene <- "GPAM"
GPAM_pLoF_genebass$burden <- "pLoF"
GPAM_mis_genebass$gene <- "GPAM"
GPAM_mis_genebass$burden <- "mis"
GPAM_synon_genebass$gene <- "GPAM"
GPAM_synon_genebass$burden <- "synon"
TNRC6B_pLoF_genebass$gene <- "TNRC6B"
TNRC6B_pLoF_genebass$burden <- "pLoF"
TNRC6B_mis_genebass$gene <- "TNRC6B"
TNRC6B_mis_genebass$burden <- "mis"
TNRC6B_synon_genebass$gene <- "TNRC6B"
TNRC6B_synon_genebass$burden <- "synon"

Genebass_sum1 <- rbind(ALB_pLoF_genebass, ALB_pLoF_genebass, ALB_mis_genebass, ALB_mis_genebass, ALB_synon_genebass, ALB_synon_genebass, ACVR2A_pLoF_genebass, ACVR2A_pLoF_genebass, ACVR2A_mis_genebass, ACVR2A_mis_genebass, ACVR2A_synon_genebass, ACVR2A_synon_genebass, CIDEB_pLoF_genebass, CIDEB_pLoF_genebass, CIDEB_mis_genebass, CIDEB_mis_genebass, CIDEB_synon_genebass, CIDEB_synon_genebass, FOXO1_pLoF_genebass, FOXO1_pLoF_genebass, FOXO1_mis_genebass, FOXO1_mis_genebass, FOXO1_synon_genebass, FOXO1_synon_genebass, GPAM_pLoF_genebass, GPAM_pLoF_genebass, GPAM_mis_genebass, GPAM_mis_genebass, GPAM_synon_genebass, GPAM_synon_genebass, TNRC6B_pLoF_genebass, TNRC6B_pLoF_genebass, TNRC6B_mis_genebass, TNRC6B_mis_genebass, TNRC6B_synon_genebass, TNRC6B_synon_genebass)

Genebass_sum1 <- Genebass_sum1 %>% rename(pval = "P-Value (SKAT-O)")
Genebass_sum1 = subset(Genebass_sum1, pval <=2.5e-8)
Genebass_sum1 <- Genebass_sum1 %>% group_by(gene, burden, Description) %>% arrange(pval) %>% slice(1:1)
write.table(Genebass_sum1,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/Genebass_sum1.csv",sep=",")

#Join together data tables from AZ & genebass

AZ_gene_sum2 <- AZ_gene_sum
AZ_gene_sum2$AA_change <- ""
AZ_gene_sum2$ConsType <- ""
AZ_gene_sum2$Variant <- ""
AZ_gene_sum2$VarType <- ""
AZ_gene_sum2 <- AZ_gene_sum2 %>% rename(Model = "ColMod")
AZ_gene_sum2$MAF <- AZ_gene_sum2$Sum_with_var / AZ_gene_sum2$Total
AZ_gene_sum2 = subset(AZ_gene_sum2, select = c("gene", "Phenotype", "Variant", "AA_change", "VarType", "ConsType", "Model", "EffectSize", "LCI", "UCI", "pval", "neglogp", "MAF", "Total", "analysis", "type", "source"))

Genebass_sum2 <- Genebass_sum1
Genebass_sum2 = subset(Genebass_sum2, select = -c(Phenotype))
Genebass_sum2 <- Genebass_sum2 %>% rename(Phenotype = "Description")
Genebass_sum2$AA_change <- ""
Genebass_sum2$ConsType <- ""
Genebass_sum2$Variant <- ""
Genebass_sum2$VarType <- ""
Genebass_sum2$neglogp <- -log10(Genebass_sum2$pval)
Genebass_sum2 <- Genebass_sum2 %>% rename(Total = "N cases")
Genebass_sum2$UCI <- as.numeric("")
Genebass_sum2$LCI <- as.numeric("")
Genebass_sum2$MAF <- as.numeric("")
Genebass_sum2 <- Genebass_sum2 %>% rename(Model = "burden")
Genebass_sum2 <- Genebass_sum2 %>% rename(EffectSize = "Beta")
Genebass_sum2$source <- "Genebass"
Genebass_sum2 <- Genebass_sum2 %>% rename(type = "analysis_id")
Genebass_sum2$analysis <- "GeneLevel"
Genebass_sum2 = subset(Genebass_sum2, select = c("gene", "Phenotype", "Variant", "AA_change", "VarType", "ConsType", "Model", "EffectSize", "LCI", "UCI", "pval", "neglogp", "MAF", "Total", "analysis", "type", "source"))

RareExome_sumtab <- rbind(AZ_gene_sum2, AZ_var_sum, Genebass_sum2)
write.table(RareExome_sumtab,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/SupTab4.csv",sep=",")

## further filtering of columns for Table 4

RareExome_sumtab2 <- RareExome_sumtab
RareExome_sumtab2 = subset(RareExome_sumtab2, select = -c(LCI, UCI, neglogp, MAF))

RareExome_sumtab2 <- RareExome_sumtab2 %>% group_by(gene, Phenotype, analysis) %>% arrange(pval) %>% slice(1:1)

RareExome_sumtab2 <- subset(RareExome_sumtab2, Phenotype =='Alanine aminotransferase' | Phenotype =='Alkaline phosphatase' | Phenotype =='Apolipoprotein A' | Phenotype =='Apolipoprotein B' | Phenotype =='Body mass index (BMI)(21001)' | Phenotype =='Cholesterol' | Phenotype =='Creatinine' | Phenotype =='Direct bilirubin' | Phenotype =='Frequency of failure to fulfil normal expectations due to drinking alcohol in last year' | Phenotype =='HDL cholesterol' | Phenotype =='Hip circumference' | Phenotype =='Impedance of whole body' | Phenotype =='LDL direct' | Phenotype =='Leg fat mass (left)' | Phenotype =='Leg fat mass (right)' | Phenotype =='Total bilirubin' | Phenotype =='Triglycerides')
write.table(RareExome_sumtab2,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/Table4.csv",sep=",")


#### --- Look for metabolite associations of variants and genes
## Some metabolite data already obtained from T2DM_KP (but filtered out above)
## Use Lotta/Pietzner omicscience.org to look up each gene
mWAS_full <- read_excel("mWAS_full.xlsx")

## remove duplicates based on per-gene and by trait (and direction), keeping variant with smallest pval
mWAS_full_rmdup_v2 <- mWAS_full %>% group_by(gene, met_full) %>% arrange(Pvalue_MA) %>% slice(1:1)
## create table to merge names of metabolites with HMDB, PubChem, and KEGG
Metab_convert <- read_excel("Metab_convert_v2.xlsx")

mWAS_full_names <- merge(mWAS_full_rmdup_v2, Metab_convert, by="met_full", all= TRUE)
mWAS_full_names <- mWAS_full_names %>% arrange(gene)
# nil with signifincant pvalue (4.9e-10)

