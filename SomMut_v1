## import replication data into R and tabulate


## MAGIC_HbA1c
MAGIC_HbA1c_header <- read.table(file = 'MAGIC_HbA1c_header.tsv', sep = '\t', header = FALSE)

MAGIC_HbA1c_ACVR2A <- read.table(file = 'MAGIC_HbA1c_ACVR2A.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_ACVR2A) <- MAGIC_HbA1c_header
MAGIC_HbA1c_ACVR2A$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_ACVR2A$HCCgene <- "ACVR2A"
MAGIC_HbA1c_ACVR2A = subset(MAGIC_HbA1c_ACVR2A, log10BF !="")
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% filter(between(base_pair_location, 148602086, 148688393))
MAGIC_HbA1c_ACVR2A$beta <- 10^MAGIC_HbA1c_ACVR2A$log10BF
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(se = standard_error)
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(pval = het_p_value)
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(chr = chromosome)
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(pos = base_pair_location)
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(ref = other_allele)
MAGIC_HbA1c_ACVR2A <- MAGIC_HbA1c_ACVR2A %>% rename(alt = effect_allele)
MAGIC_HbA1c_ACVR2A$EAF <- ""
MAGIC_HbA1c_ACVR2A = subset(MAGIC_HbA1c_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

MAGIC_HbA1c_ALB <- read.table(file = 'MAGIC_HbA1c_ALB.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_ALB) <- MAGIC_HbA1c_header
MAGIC_HbA1c_ALB$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_ALB$HCCgene <- "ALB"
MAGIC_HbA1c_ALB = subset(MAGIC_HbA1c_ALB, beta !="")
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% filter(between(base_pair_location, 74262831, 74287129))
MAGIC_HbA1c_ALB$beta <- 10^MAGIC_HbA1c_ALB$log10BF
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(se = standard_error)
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(pval = het_p_value)
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(chr = chromosome)
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(pos = base_pair_location)
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(ref = other_allele)
MAGIC_HbA1c_ALB <- MAGIC_HbA1c_ALB %>% rename(alt = effect_allele)
MAGIC_HbA1c_ALB$EAF <- ""
MAGIC_HbA1c_ALB = subset(MAGIC_HbA1c_ALB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

MAGIC_HbA1c_CIDEB <- read.table(file = 'MAGIC_HbA1c_CIDEB.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_CIDEB) <- MAGIC_HbA1c_header
MAGIC_HbA1c_CIDEB$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_CIDEB$HCCgene <- "CIDEB"
MAGIC_HbA1c_CIDEB = subset(MAGIC_HbA1c_CIDEB, beta !="")
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% filter(between(base_pair_location, 24774302, 24780636))
MAGIC_HbA1c_CIDEB$beta <- 10^MAGIC_HbA1c_CIDEB$log10BF
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(se = standard_error)
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(pval = het_p_value)
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(chr = chromosome)
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(pos = base_pair_location)
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(ref = other_allele)
MAGIC_HbA1c_CIDEB <- MAGIC_HbA1c_CIDEB %>% rename(alt = effect_allele)
MAGIC_HbA1c_CIDEB$EAF <- ""
MAGIC_HbA1c_CIDEB = subset(MAGIC_HbA1c_CIDEB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

MAGIC_HbA1c_FOXO1 <- read.table(file = 'MAGIC_HbA1c_FOXO1.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_FOXO1) <- MAGIC_HbA1c_header
MAGIC_HbA1c_FOXO1$trait <- "MAGIC_HbA1c"
