MAGIC_HbA1c_MTARC1 <- read.table(file = 'MAGIC_HbA1c_MTARC1.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_MTARC1) <- MAGIC_HbA1c_header
MAGIC_HbA1c_MTARC1$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_MTARC1$HCCgene <- "MTARC1"
MAGIC_HbA1c_MTARC1 = subset(MAGIC_HbA1c_MTARC1, log10BF !="")
MAGIC_HbA1c_MTARC1$beta <- 10^MAGIC_HbA1c_MTARC1$log10BF
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(se = standard_error)
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(pval = het_p_value)
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(chr = chromosome)
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(pos = base_pair_location)
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(ref = other_allele)
MAGIC_HbA1c_MTARC1 <- MAGIC_HbA1c_MTARC1 %>% rename(alt = effect_allele)
MAGIC_HbA1c_MTARC1$EAF <- ""
MAGIC_HbA1c_MTARC1 = subset(MAGIC_HbA1c_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

