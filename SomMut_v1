BBJ_HbA1c_TM6SF2 <- BBJ_HbA1c_TM6SF2 %>% rename(beta = BETA)
BBJ_HbA1c_TM6SF2 <- BBJ_HbA1c_TM6SF2 %>% rename(se = SE)
