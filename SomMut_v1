./tabtk/tabtk grep -f 2 "220970028" /Users/jakemann/UKBB_sumstats/GCST90013405_buildGRCh37.tsv > /Users/jakemann/UKBB_sumstats/ALT_metan_MTARC1.tsv

./tabtk/tabtk grep -f 4 "220970028" /Users/jakemann/UKBB_sumstats/SpracklenCN_prePMID_T2D_ALL_Primary.txt > /Users/jakemann/UKBB_sumstats/Sprack_T2DM_MTARC1.tsv

./tabtk/tabtk grep -f 3 "220970028" /Users/jakemann/UKBB_sumstats/MAGIC1000G_HbA1c_TA.tsv > /Users/jakemann/UKBB_sumstats/MAGIC_HbA1c_MTARC1.tsv


