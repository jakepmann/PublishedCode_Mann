Anstee_MTARC1 <- Anstee_MTARC1 %>% rename(pval = p_value)
Anstee_MTARC1 <- Anstee_MTARC1 %>% rename(chr = chromosome)
Anstee_MTARC1 <- Anstee_MTARC1 %>% rename(pos = base_pair_location)
Anstee_MTARC1 <- Anstee_MTARC1 %>% rename(ref = other_allele)
Anstee_MTARC1 <- Anstee_MTARC1 %>% rename(alt = effect_allele)
Anstee_MTARC1$EAF <- ""
Anstee_MTARC1 = subset(Anstee_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))


FinnGen_ALCOLIVER_MTARC1 <- read.table(file = 'FinnGen_ALCOLIVER_MTARC1.tsv', sep = '\t', header = FALSE)
colnames(FinnGen_ALCOLIVER_MTARC1) <- FinnGen_ALCOLIVER_header
FinnGen_ALCOLIVER_MTARC1$trait <- "FinnGen_ALCOLIVER"
FinnGen_ALCOLIVER_MTARC1$HCCgene <- "MTARC1"
FinnGen_ALCOLIVER_MTARC1 = subset(FinnGen_ALCOLIVER_MTARC1, beta !="")
FinnGen_ALCOLIVER_MTARC1 <- FinnGen_ALCOLIVER_MTARC1 %>% rename(se = sebeta)
FinnGen_ALCOLIVER_MTARC1 <- FinnGen_ALCOLIVER_MTARC1 %>% rename(pval = pval)
names(FinnGen_ALCOLIVER_MTARC1)[names(FinnGen_ALCOLIVER_MTARC1) == '#chrom'] <- 'chr'
FinnGen_ALCOLIVER_MTARC1 <- FinnGen_ALCOLIVER_MTARC1 %>% rename(EAF = maf)
FinnGen_ALCOLIVER_MTARC1 = subset(FinnGen_ALCOLIVER_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

