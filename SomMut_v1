Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(beta = odds_ratio)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(se = standard_error)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(pval = p_value)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(chr = chromosome)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(pos = base_pair_location)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(ref = other_allele)
Anstee_NEAT1 <- Anstee_NEAT1 %>% rename(alt = effect_allele)
Anstee_NEAT1$EAF <- ""
Anstee_NEAT1 = subset(Anstee_NEAT1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

