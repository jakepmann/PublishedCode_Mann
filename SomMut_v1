## make summary tables per gene

ACVR2A_summary <- rbind(LiverDis_ACVR2A, ALD_ACVR2A, Cirrhosis_ACVR2A, ALT_ACVR2A, AST_ACVR2A, Fibrosis_ACVR2A, NAFLD_ACVR2A, HbA1c_ACVR2A, FinnGen_HCC_ACVR2A, FinnGen_Cirrhosis_ACVR2A, FinnGen_NAFLD_ACVR2A, FinnGen_ALCOLIVER_ACVR2A, Liu_LiverFat_ACVR2A, Anstee_ACVR2A)
ALB_summary <- rbind(LiverDis_ALB, ALD_ALB, ALT_ALB, AST_ALB, Cirrhosis_ALB, Fibrosis_ALB, NAFLD_ALB, HbA1c_ALB, FinnGen_HCC_ALB, FinnGen_Cirrhosis_ALB, FinnGen_NAFLD_ALB, FinnGen_ALCOLIVER_ALB, Liu_LiverFat_ALB, Anstee_ALB)
CIDEB_summary <- rbind(LiverDis_CIDEB, AST_CIDEB, ALD_CIDEB, ALT_CIDEB, Cirrhosis_CIDEB, Fibrosis_CIDEB, NAFLD_CIDEB, HbA1c_CIDEB, FinnGen_HCC_CIDEB, FinnGen_Cirrhosis_CIDEB, FinnGen_NAFLD_CIDEB, FinnGen_ALCOLIVER_CIDEB, Liu_LiverFat_CIDEB, Anstee_CIDEB)
FOXO1_summary <- rbind(LiverDis_FOXO1, AST_FOXO1, ALD_FOXO1, ALT_FOXO1, Cirrhosis_FOXO1, Fibrosis_FOXO1, NAFLD_FOXO1, HbA1c_FOXO1, FinnGen_HCC_FOXO1, FinnGen_Cirrhosis_FOXO1, FinnGen_NAFLD_FOXO1, FinnGen_ALCOLIVER_FOXO1, Liu_LiverFat_FOXO1, Anstee_FOXO1)
GPAM_summary <- rbind(LiverDis_GPAM, ALD_GPAM, AST_GPAM, ALT_GPAM, Fibrosis_GPAM, Cirrhosis_GPAM, NAFLD_GPAM, HbA1c_GPAM, FinnGen_HCC_GPAM, FinnGen_Cirrhosis_GPAM, FinnGen_NAFLD_GPAM, FinnGen_ALCOLIVER_GPAM, Liu_LiverFat_GPAM, Anstee_GPAM)
NEAT1_summary <- rbind(LiverDis_NEAT1, ALD_NEAT1, AST_NEAT1, ALT_NEAT1, Fibrosis_NEAT1, Cirrhosis_NEAT1, NAFLD_NEAT1, HbA1c_NEAT1, FinnGen_HCC_NEAT1, FinnGen_Cirrhosis_NEAT1, FinnGen_NAFLD_NEAT1, FinnGen_ALCOLIVER_NEAT1, Liu_LiverFat_NEAT1, Anstee_NEAT1)
TNRC6B_summary <- rbind(LiverDis_TNRC6B, ALD_TNRC6B, AST_TNRC6B, ALT_TNRC6B, Fibrosis_TNRC6B, Cirrhosis_TNRC6B, NAFLD_TNRC6B, HbA1c_TNRC6B, FinnGen_HCC_TNRC6B, FinnGen_Cirrhosis_TNRC6B, FinnGen_NAFLD_TNRC6B, FinnGen_ALCOLIVER_TNRC6B, Liu_LiverFat_TNRC6B, Anstee_TNRC6B)

GWAS_sumstat_sumtab <- rbind(ACVR2A_summary, ALB_summary, CIDEB_summary, FOXO1_summary, GPAM_summary, NEAT1_summary, TNRC6B_summary)


# need to convert FinnGen positions from GRCh38 > 37
GRCh38 = subset(GWAS_sumstat_sumtab, trait == "FinnGen_Cirrhosis" | trait == "FinnGen_NAFLD" | trait == "FinnGen_HCC" | trait == "FinnGen_ALCOLIVER")
write.table(GRCh38,file="GRCh38.csv",sep=",")

#write table and then convert using ensembl web converter
# re-import FinnGen results now in 37 format and bind with other data
GRCh37 = subset(GWAS_sumstat_sumtab, trait != "FinnGen_Cirrhosis" & trait != "FinnGen_NAFLD" & trait != "FinnGen_HCC" & trait != "FinnGen_ALCOLIVER")

FinnGen_GRCh37 <- read_excel("FinnGen_GRCh37.xlsx")

GWAS_sumstat_sumtab <-rbind(FinnGen_GRCh37, GRCh37)
GWAS_sumstat_sumtab$neglogp <- -(log10(GWAS_sumstat_sumtab$pval))

## albumin data has included some positions from chr14! so will need to remove those
GWAS_sumstat_sumtab_drop = subset(GWAS_sumstat_sumtab, HCCgene=="ALB" & chr=="14")
GWAS_sumstat_sumtab <- anti_join(GWAS_sumstat_sumtab, GWAS_sumstat_sumtab_drop, by=c("HCCgene", "chr", "pos"))

## make summary figures
library(ggplot2)
library(cowplot)


GWAS_sumstat_sumtab <- GWAS_sumstat_sumtab %>% group_by(trait) %>% arrange(chr, pos) %>% mutate(bp = row_number())

cbPalette <- c("#999999","#E69F00","#56B4E9","#009E73","#F0E442","#0072B2","#D55E00","#CC79A7")

ALT_fig <- ggplot(subset(GWAS_sumstat_sumtab, trait=="ALT"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0,32), breaks=seq(0, 30, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "ALT (UK BioBank: n=420,449)")
pdf(file="Fig_ALT.pdf",width=10,height=3)
ALT_fig
dev.off()

AST_fig <- ggplot(subset(GWAS_sumstat_sumtab, trait=="AST"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "AST (UK BioBank: n=419,034)")
pdf(file="Fig_AST_fig.pdf",width=10,height=3)
AST_fig
dev.off()


