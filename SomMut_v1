#### --- move back to R for manipulation of searched GWAS data

## import files, give headers, and merge

library(dplyr)



## UKBB LiverDis
LiverDis_header <- read.table(file = 'LiverDis_header.tsv', sep = '\t', header = FALSE)

LiverDis_ACVR2A <- read.table(file = 'LiverDis_UKBB_ACVR2A.tsv', sep = '\t', header = FALSE)
colnames(LiverDis_ACVR2A) <- LiverDis_header
LiverDis_ACVR2A$trait <- "LiverDis"
LiverDis_ACVR2A$HCCgene <- "ACVR2A"
LiverDis_ACVR2A = subset(LiverDis_ACVR2A, beta_EUR !="")
LiverDis_ACVR2A <- LiverDis_ACVR2A %>% filter(between(pos, 148602086, 148688393))
LiverDis_ACVR2A <- LiverDis_ACVR2A %>% rename(beta = beta_EUR)
LiverDis_ACVR2A <- LiverDis_ACVR2A %>% rename(se = se_EUR)
LiverDis_ACVR2A <- LiverDis_ACVR2A %>% rename(pval = pval_EUR)
LiverDis_ACVR2A <- LiverDis_ACVR2A %>% rename(EAF = af_controls_EUR)
LiverDis_ACVR2A = subset(LiverDis_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

LiverDis_ALB <- read.table(file = 'LiverDis_UKBB_ALB.tsv', sep = '\t', header = FALSE)
colnames(LiverDis_ALB) <- LiverDis_header
LiverDis_ALB$trait <- "LiverDis"
LiverDis_ALB$HCCgene <- "ALB"
LiverDis_ALB = subset(LiverDis_ALB, beta_EUR !="")
LiverDis_ALB <- LiverDis_ALB %>% filter(between(pos, 74262831, 74287129))
LiverDis_ALB <- LiverDis_ALB %>% rename(beta = beta_EUR)
LiverDis_ALB <- LiverDis_ALB %>% rename(se = se_EUR)
LiverDis_ALB <- LiverDis_ALB %>% rename(pval = pval_EUR)
LiverDis_ALB <- LiverDis_ALB %>% rename(EAF = af_controls_EUR)
LiverDis_ALB = subset(LiverDis_ALB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

LiverDis_CIDEB <- read.table(file = 'LiverDis_UKBB_CIDEB.tsv', sep = '\t', header = FALSE)
colnames(LiverDis_CIDEB) <- LiverDis_header
LiverDis_CIDEB$trait <- "LiverDis"
LiverDis_CIDEB$HCCgene <- "CIDEB"
LiverDis_CIDEB = subset(LiverDis_CIDEB, beta_EUR !="")
LiverDis_CIDEB <- LiverDis_CIDEB %>% filter(between(pos, 24774302, 24780636))
LiverDis_CIDEB <- LiverDis_CIDEB %>% rename(beta = beta_EUR)
