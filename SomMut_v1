colnames(ALT_metan_ACVR2A) <- ALT_metan_header
ALT_metan_ACVR2A$trait <- "ALT_metan"
ALT_metan_ACVR2A$HCCgene <- "ACVR2A"
ALT_metan_ACVR2A = subset(ALT_metan_ACVR2A, beta !="")
