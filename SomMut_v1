Anstee_ALB <- Anstee_ALB %>% rename(pos = base_pair_location)
Anstee_ALB <- Anstee_ALB %>% rename(ref = other_allele)
Anstee_ALB <- Anstee_ALB %>% rename(alt = effect_allele)
Anstee_ALB$EAF <- ""
Anstee_ALB = subset(Anstee_ALB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

