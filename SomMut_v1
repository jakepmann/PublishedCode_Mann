
######
###### ---- analysis for germline mutation in genes with somatic mutants that are enriched in ARLD/NAFLD from https://www.nature.com/articles/s41586-021-03974-6


###### ----
###### ----
###### ----
###### ---- Remove duplicates from 258 --> 129 unique variants

## convert variants from hg19 to hg38 using ensembl assembly converter (using BED format)
## annotate with VEP using web tool
## Also ran with SnpEff, which gave same variants / interpretation so will take VEP forward

library(readxl)
library(dplyr)
library(Qtlizer)
library(ggplot2)
library(cowplot)
library(reshape2)
library(phenoscanner)
library(biomaRt)
library(tidyverse)
library(readr)

###### ---- Import annotated variants into R 4.0.2

HCCvars_VEP_annot <- read_excel("SomMut_VEP_res_v4.xlsx")

## filter based on ensembl canonical transcripts
HCCvars_VEP_annot = subset(HCCvars_VEP_annot, Feature=="ENST00000241416.12" | Feature =="ENST00000295897.9" | Feature =="ENST00000554411.5" | Feature =="ENST00000379561.6" | Feature =="ENST00000348367.9" | Feature =="ENST00000501122.2" | Feature =="ENST00000454349.7")
write.table(HCCvars_VEP_annot,file="HCCvars_VEP_annot.csv",sep=",")

## NEAT1 not annotated with allele frequencines by VEP as this only contains gnomAD exomes, so separately download and import allele frequencines for NEAT1
gnomAD_NEAT1_AF <- read_excel("gnomAD_NEAT1_AF.xlsx") ## this is GRCh37
## import alleles for NEAT1 in GRCh37
SomMut_vars_raw_37 <- read_excel("SomMut_vars_raw_37.xlsx")
## merge for NEAT1
NEAT1_AF_37 <- merge(gnomAD_NEAT1_AF, SomMut_vars_raw_37, by=c("Chromosome","Position","Alternate"))
## no overlap, therefore no somatic variants in NEAT1 identified in gnomAD

HCCvars_VEP_annot$AF_sum <- ifelse(HCCvars_VEP_annot$gnomAD_AF =="-", "Nil", "<.1%")


## make table of raw VEP output merged with raw variant input
RawVars_VEP_input <- read_excel("SomMut_vars_EnsmblFormat.xlsx")
RawVars_VEP_suptab <- merge(RawVars_VEP_input, HCCvars_VEP_annot)
RawVars_VEP_suptab = subset(RawVars_VEP_suptab, select = -c(var, alt_VEP, StrandText, Len_ref, Len_alt, Diff, Type, End_coord, VEP_working, VEP_input))
write.table(RawVars_VEP_suptab,file="SupTab2.csv",sep=",")

## make summary table of all variants from VEP

All_VEP_Cons <- HCCvars_VEP_annot %>% group_by(Consequence) %>% summarise(Overall = n())
All_VEP_Imp <- HCCvars_VEP_annot %>% group_by(IMPACT) %>% summarise(Overall = n())
All_VEP_AF <- HCCvars_VEP_annot %>% group_by(AF_sum) %>% summarise(Overall = n())

Gene_VEP_Cons <- HCCvars_VEP_annot %>% group_by(SYMBOL,Consequence) %>% summarise(n = n())
Gene_VEP_Cons2 <- dcast(Gene_VEP_Cons, Consequence~SYMBOL, sum)
Gene_VEP_Imp <- HCCvars_VEP_annot %>% group_by(SYMBOL,IMPACT) %>% summarise(n = n())
Gene_VEP_Imp2 <- dcast(Gene_VEP_Imp, IMPACT~SYMBOL, sum)
Gene_VEP_AF <- HCCvars_VEP_annot %>% group_by(AF_sum, SYMBOL) %>% summarise(n = n())
Gene_VEP_AF2 <- dcast(Gene_VEP_AF, AF_sum~SYMBOL, sum)

Sum_VEP_Cons <- cbind(Gene_VEP_Cons2, All_VEP_Cons[,2, drop=FALSE])
Sum_VEP_Cons$Prop <- (Sum_VEP_Cons$Overall/(sum(Sum_VEP_Cons$Overall))*100)
Sum_VEP_Cons$text <- paste(Sum_VEP_Cons$Overall, round(Sum_VEP_Cons$Prop, digits=1), sep = " (", collapse = NULL)
Sum_VEP_Cons$text <- paste(Sum_VEP_Cons$text, "", sep = "%)", collapse = NULL)
Sum_VEP_Cons = subset(Sum_VEP_Cons, select = -c(Overall,Prop))
Sum_VEP_Cons <- Sum_VEP_Cons %>% rename(Overall = text)
Sum_VEP_Cons <- Sum_VEP_Cons %>% rename(Var = Consequence)

Sum_VEP_Imp <- cbind(Gene_VEP_Imp2, All_VEP_Imp[,2, drop=FALSE])
Sum_VEP_Imp$Prop <- (Sum_VEP_Imp$Overall/(sum(Sum_VEP_Imp$Overall))*100)
Sum_VEP_Imp$text <- paste(Sum_VEP_Imp$Overall, round(Sum_VEP_Imp$Prop, digits=1), sep = " (", collapse = NULL)
Sum_VEP_Imp$text <- paste(Sum_VEP_Imp$text, "", sep = "%)", collapse = NULL)
Sum_VEP_Imp = subset(Sum_VEP_Imp, select = -c(Overall,Prop))
Sum_VEP_Imp <- Sum_VEP_Imp %>% rename(Overall = text)
Sum_VEP_Imp <- Sum_VEP_Imp %>% rename(Var = IMPACT)

Sum_VEP_AF <- cbind(Gene_VEP_AF2, All_VEP_AF[,2, drop=FALSE])
Sum_VEP_AF$Prop <- (Sum_VEP_AF$Overall/(sum(Sum_VEP_AF$Overall))*100)
Sum_VEP_AF$text <- paste(Sum_VEP_AF$Overall, round(Sum_VEP_AF$Prop, digits=1), sep = " (", collapse = NULL)
Sum_VEP_AF$text <- paste(Sum_VEP_AF$text, "", sep = "%)", collapse = NULL)
Sum_VEP_AF = subset(Sum_VEP_AF, select = -c(Overall,Prop))
Sum_VEP_AF <- Sum_VEP_AF %>% rename(Overall = text)
Sum_VEP_AF <- Sum_VEP_AF %>% rename(Var = AF_sum)

VEP_summary_1 <- rbind(Sum_VEP_Cons, Sum_VEP_Imp, Sum_VEP_AF)
write.table(VEP_summary_1,file="00_SomMut_JHepRep_v1/Table1.csv",sep=",")

######## ---
## for the few variants that do have an rsID, try running through phenoscanner
Existing_var  <- read_excel("Existing_var.xlsx")

Existing_var_res <- phenoscanner(snpquery= Existing_var$Existing_var)
## nil found
## mannually search common metabolic disease portal for these

