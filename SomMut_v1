BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(EAF = Frq)
BBJ_HbA1c_HSD17B13 = subset(BBJ_HbA1c_HSD17B13, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

HSD17B13_summary <- rbind(LiverDis_HSD17B13, ALD_HSD17B13, Cirrhosis_HSD17B13, ALT_HSD17B13, AST_HSD17B13, Fibrosis_HSD17B13, NAFLD_HSD17B13, HbA1c_HSD17B13, FinnGen_HCC_HSD17B13, FinnGen_Cirrhosis_HSD17B13, FinnGen_NAFLD_HSD17B13, FinnGen_ALCOLIVER_HSD17B13, Liu_LiverFat_HSD17B13, Anstee_HSD17B13)
HSD17B13_rep_summary <- rbind(MAGIC_HbA1c_HSD17B13, Sprack_T2DM_HSD17B13, ALT_metan_HSD17B13, BBJ_ALT_HSD17B13, BBJ_HbA1c_HSD17B13)
HSD17B13_overall_summary <- rbind(HSD17B13_summary, HSD17B13_rep_summary)

