### import and manipulte other summary stats

## Anstee NAFLD Dx
Anstee_header <- read.table(file = 'Anstee_header.tsv', sep = '\t', header = FALSE)

Anstee_ACVR2A <- read.table(file = 'Anstee_ACVR2A.tsv', sep = '\t', header = FALSE)
colnames(Anstee_ACVR2A) <- Anstee_header
Anstee_ACVR2A$trait <- "Anstee_NAFLD_Dx"
Anstee_ACVR2A$HCCgene <- "ACVR2A"
Anstee_ACVR2A = subset(Anstee_ACVR2A, odds_ratio !="")
Anstee_ACVR2A <- Anstee_ACVR2A %>% filter(between(base_pair_location, 148602086, 148688393))
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(beta = odds_ratio)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(se = standard_error)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(pval = p_value)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(chr = chromosome)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(pos = base_pair_location)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(ref = other_allele)
Anstee_ACVR2A <- Anstee_ACVR2A %>% rename(alt = effect_allele)
Anstee_ACVR2A$EAF <- ""
Anstee_ACVR2A = subset(Anstee_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

Anstee_ALB <- read.table(file = 'Anstee_ALB.tsv', sep = '\t', header = FALSE)
colnames(Anstee_ALB) <- Anstee_header
Anstee_ALB$trait <- "Anstee_NAFLD_Dx"
Anstee_ALB$HCCgene <- "ALB"
Anstee_ALB = subset(Anstee_ALB, odds_ratio !="")
Anstee_ALB <- Anstee_ALB %>% filter(between(base_pair_location, 74262831, 74287129))
