BBJ_HbA1c_TM6SF2 <- BBJ_HbA1c_TM6SF2 %>% rename(EAF = Frq)
BBJ_HbA1c_TM6SF2 = subset(BBJ_HbA1c_TM6SF2, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

TM6SF2_summary <- rbind(LiverDis_TM6SF2, ALD_TM6SF2, Cirrhosis_TM6SF2, ALT_TM6SF2, AST_TM6SF2, Fibrosis_TM6SF2, NAFLD_TM6SF2, HbA1c_TM6SF2, FinnGen_HCC_TM6SF2, FinnGen_Cirrhosis_TM6SF2, FinnGen_NAFLD_TM6SF2, FinnGen_ALCOLIVER_TM6SF2, Liu_LiverFat_TM6SF2, Anstee_TM6SF2)
TM6SF2_rep_summary <- rbind(MAGIC_HbA1c_TM6SF2, Sprack_T2DM_TM6SF2, ALT_metan_TM6SF2, BBJ_ALT_TM6SF2, BBJ_HbA1c_TM6SF2)
TM6SF2_overall_summary <- rbind(TM6SF2_summary, TM6SF2_rep_summary)

