MAGIC_HbA1c_PNPLA3 <- read.table(file = 'MAGIC_HbA1c_PNPLA3.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_PNPLA3) <- MAGIC_HbA1c_header
MAGIC_HbA1c_PNPLA3$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_PNPLA3$HCCgene <- "PNPLA3"
MAGIC_HbA1c_PNPLA3 = subset(MAGIC_HbA1c_PNPLA3, log10BF !="")
MAGIC_HbA1c_PNPLA3$beta <- 10^MAGIC_HbA1c_PNPLA3$log10BF
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(se = standard_error)
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(pval = het_p_value)
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(chr = chromosome)
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(pos = base_pair_location)
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(ref = other_allele)
MAGIC_HbA1c_PNPLA3 <- MAGIC_HbA1c_PNPLA3 %>% rename(alt = effect_allele)
MAGIC_HbA1c_PNPLA3$EAF <- ""
MAGIC_HbA1c_PNPLA3 = subset(MAGIC_HbA1c_PNPLA3, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

