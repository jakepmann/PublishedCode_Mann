MAGIC_HbA1c_FOXO1$EAF <- ""
MAGIC_HbA1c_FOXO1 = subset(MAGIC_HbA1c_FOXO1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

