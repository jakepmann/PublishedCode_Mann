FinnGen_Cirrhosis_ALB <- FinnGen_Cirrhosis_ALB %>% filter(between(pos, 73397114, 73421482))
FinnGen_Cirrhosis_ALB <- FinnGen_Cirrhosis_ALB %>% rename(se = sebeta)
FinnGen_Cirrhosis_ALB <- FinnGen_Cirrhosis_ALB %>% rename(pval = pval)
names(FinnGen_Cirrhosis_ALB)[names(FinnGen_Cirrhosis_ALB) == '#chrom'] <- 'chr'
FinnGen_Cirrhosis_ALB <- FinnGen_Cirrhosis_ALB %>% rename(EAF = maf)
FinnGen_Cirrhosis_ALB = subset(FinnGen_Cirrhosis_ALB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

