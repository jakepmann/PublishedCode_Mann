BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(beta = BETA)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(se = SE)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(pval = P)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(chr = CHR)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(pos = POS)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(ref = REF)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(alt = ALT)
BBJ_HbA1c_MTARC1 <- BBJ_HbA1c_MTARC1 %>% rename(EAF = Frq)
BBJ_HbA1c_MTARC1 = subset(BBJ_HbA1c_MTARC1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

MTARC1_summary <- rbind(LiverDis_MTARC1, ALD_MTARC1, Cirrhosis_MTARC1, ALT_MTARC1, AST_MTARC1, Fibrosis_MTARC1, NAFLD_MTARC1, HbA1c_MTARC1, FinnGen_HCC_MTARC1, FinnGen_Cirrhosis_MTARC1, FinnGen_NAFLD_MTARC1, FinnGen_ALCOLIVER_MTARC1, Liu_LiverFat_MTARC1, Anstee_MTARC1)
MTARC1_rep_summary <- rbind(MAGIC_HbA1c_MTARC1, Sprack_T2DM_MTARC1, ALT_metan_MTARC1, BBJ_ALT_MTARC1, BBJ_HbA1c_MTARC1)
MTARC1_overall_summary <- rbind(MTARC1_summary, MTARC1_rep_summary)

## filter to remove incorrect variants
PNPLA3_overall_summary = subset(PNPLA3_overall_summary, chr =="22")
TM6SF2_overall_summary = subset(TM6SF2_overall_summary, chr =="19")
GWASsig_summary <- rbind(PNPLA3_overall_summary, TM6SF2_overall_summary, HSD17B13_overall_summary, MTARC1_overall_summary)
GWASsig_summary$neglogp <- -(log10(GWASsig_summary$pval))

## filter data from GPAM for top variant for each trait and then bind with the GWAS sig summary table
GPAM_sumstat_top = subset(GWAS_sumstat_full, HCCgene =="GPAM")
GPAM_sumstat_top <- GPAM_sumstat_top %>% group_by(trait) %>% arrange(pval) %>% slice(1:1)

GPAM_comparison_tab <- rbind(GWASsig_summary, GPAM_sumstat_top)
GPAM_comparison_tab <- GPAM_comparison_tab %>% arrange(trait, HCCgene)
write.table(GPAM_comparison_tab,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/GPAM_comparison_tab.csv",sep=",")

