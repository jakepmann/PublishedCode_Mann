BBJ_ALT_PNPLA3 <- read.table(file = 'BBJ_ALT_PNPLA3.tsv', sep = '\t', header = FALSE)
colnames(BBJ_ALT_PNPLA3) <- BBJ_ALT_header
BBJ_ALT_PNPLA3$trait <- "BBJ_ALT"
BBJ_ALT_PNPLA3$HCCgene <- "PNPLA3"
