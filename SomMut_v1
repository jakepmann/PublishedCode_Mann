dev.off()


###### ------
###### ------
###### ------
###### ------
###### ------
###### ------ perform a more broad gene-based PheWAS using Phenoscanner and then common metabolic diseases knowledge portal
###### ------
## Phenoscanner


res1 <- phenoscanner(genequery="ACVR2A")
results1 <- as.data.frame(res1[["results"]])

res2 <- phenoscanner(genequery="ALB")
results2 <- as.data.frame(res2[["results"]])

res3 <- phenoscanner(genequery="CIDEB")
results3 <- as.data.frame(res3[["results"]])

res4 <- phenoscanner(genequery="FOXO1")
results4 <- as.data.frame(res4[["results"]])

res5 <- phenoscanner(genequery="GPAM")
results5 <- as.data.frame(res5[["results"]])

res6 <- phenoscanner(genequery="NEAT1")
results6 <- as.data.frame(res6[["results"]])

res7 <- phenoscanner(genequery="TNRC6B")
results7 <- as.data.frame(res7[["results"]])

results_all <- rbind(results1, results2, results3, results4, results5, results6, results7)
write.table(results_all,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/results_all.csv",sep=",")



## drop very rare (or very common variants, using 1% as threshold)

# need to make them numeric first
cols.num <- c("afr", "amr", "eas", "eur", "sas", "n_cases")
results_all[cols.num] <- sapply(results_all[cols.num],as.numeric)

results_all_drop = results_all
results_all_drop$low_freq <- ifelse(results_all_drop$afr <=.01 & results_all_drop$amr <=.01 & results_all_drop$eas <=.01 & results_all_drop$eur <=.02 & results_all_drop$sas <=.01, "Y", "N")
results_all_drop$high_freq <- ifelse(results_all_drop$afr >=.99 & results_all_drop$amr >=.99 & results_all_drop$eas >=.99 & results_all_drop$eur >=.99 & results_all_drop$sas >=.99, "Y", "N")
results_all_drop = subset(results_all_drop, low_freq=="Y" | high_freq=="Y")
results_all <- anti_join(results_all, results_all_drop, by="hg38_coordinates")

## drop associations where there are very few cases for case-control traits

results_all_drop_case = subset(results_all, unit=="risk diff")
results_all_drop_case = subset(results_all_drop_case, n_cases <=19)
results_all <- anti_join(results_all, results_all_drop_case, by="hg38_coordinates")

## remove duplicates based on per-gene and by trait (and direction), keeping variant with smallest pval
GeneLevel_rmdup <- results_all %>% group_by(gene) %>% arrange(p) %>% distinct(trait, direction, .keep_all= TRUE)

## Filter for relevant traits

GeneLevel_filt1 = subset(GeneLevel_rmdup, trait =="Alcohol intake frequency" | trait =="Ascites" | trait =="Atherosclerosis" | trait =="Atrioventricular and left bundle-branch block" | trait =="Basal metabolic rate" | trait =="Body mass index" | trait =="Cause of death: abdominal aortic aneurysm, ruptured" | trait =="Cause of death: acute vascular disorders of intestine" | trait =="Cause of death: alcoholic cirrhosis of liver" | trait =="Cause of death: alcoholic hepatic failure" | trait =="Cause of death: alcoholic hepatitis" | trait =="Cause of death: alcoholic liver disease, unspecified" | trait =="Cause of death: atrial fibrillation and flutter" | trait =="Cause of death: b-cell lymphoma, unspecified" | trait =="Cause of death: cardiomegaly" | trait =="Cause of death: cardiomyopathy, unspecified" | trait =="Cause of death: cerebral infarction, unspecified" | trait =="Cause of death: congestive heart failure" | trait =="Cause of death: dilated cardiomyopathy")

GeneLevel_filt2 = subset(GeneLevel_rmdup, trait =="Cause of death: dissection of aorta" | trait =="Cause of death: fatty liver" | trait =="Cause of death: heart failure, unspecified" | trait =="Cause of death: hypertensive heart disease with heart failure" | trait =="Cause of death: hypertensive heart disease without heart failure" | trait =="Cause of death: intracerebral haemorrhage, unspecified" | trait =="Cause of death: intracranial haemorrhage, unspecified" | trait =="Cause of death: intrahepatic bile duct carcinoma" | trait =="Cause of death: ischaemic cardiomyopathy" | trait =="Cause of death: other and unspecified cirrhosis of liver" | trait =="Cholesterol" | trait =="Cholesterol ldl" | trait =="Cholesterol total" | trait =="Chronic viral hepatitis" | trait =="Coronary artery disease" | trait =="Diabetes diagnosed by doctor" | trait =="Diabetes mellitus in pregnancy" | trait =="Diastolic blood pressure" | trait =="Elevated blood glucose level" | trait =="Glomerular filtration rate creatinine" | trait =="HDL cholesterol" | trait =="High density lipoprotein" | trait =="Hip circumference")

GeneLevel_filt3 = subset(GeneLevel_rmdup, trait =="Hip circumference adjusted for BMI" | trait =="Hypertensive renal disease" | trait =="LDL cholesterol" | trait =="LDL cholesterol levels" | trait =="Lipoproteins vldl" | trait =="Low density lipoprotein" | trait =="Malignant neoplasm of liver and intrahepatic bile ducts" | trait =="Non-insulin-dependent diabetes mellitus" | trait =="Other diseases of biliary tract" | trait =="Other diseases of liver" | trait =="Pulse pressure" | trait =="Self-reported cardiomyopathy" | trait =="Self-reported diabetes" | trait =="Self-reported gastrointestinal bleeding" | trait =="Self-reported gestational diabetes" | trait =="Self-reported heart failure or pulmonary odema" | trait =="Systolic blood pressure" | trait =="Total cholesterol" | trait =="Total cholesterol levels" | trait =="Triglycerides" | trait =="Unspecified jaundice" | trait =="Waist circumference" | trait =="Whole body fat mass")

GeneLevel_filt <- rbind(GeneLevel_filt1, GeneLevel_filt2, GeneLevel_filt3)
GeneLevel_filt <- GeneLevel_filt %>% group_by(gene) %>% arrange(gene, p)


## drop few specific variants that didn't have automatic annotation with allele frequencies but are rare (<1%)
GeneLevel_VarAnnot5 = subset(GeneLevel_filt, rsid !="rs577175003" & rsid !="rs577175003" & rsid !="rs532147338" & rsid !="13:41240039_C_G" & rsid !="rs11195836")


## Note that 13:41240039_C_G (G104A) in FOXO1 has a suggestive association with cholangiocarcinoma but, sub GWAS-sig and unable to validate in any other dataset. From FinGen: https://r5.finngen.fi/variant/13-40373690-T-C

GeneLevel_SumTab = subset(GeneLevel_VarAnnot5, select = c("gene", "rsid", "a1", "a2", "afr", "amr", "eas", "eur", "sas", "trait", "beta", "se", "p", "hgnc", "study", "n"))

cols.num2 <- c("p", "beta", "se")
GeneLevel_SumTab[cols.num2] <- sapply(GeneLevel_SumTab[cols.num2],as.numeric)

GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(Query_gene = gene)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(Nearest_gene = hgnc)
GeneLevel_SumTab$var2 <- paste(GeneLevel_SumTab$rsid, GeneLevel_SumTab$a1, sep = " ", collapse = NULL)
GeneLevel_SumTab$var2 <- paste(GeneLevel_SumTab$var2, GeneLevel_SumTab$a2, sep = ">", collapse = NULL)

GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(AFR_AF = afr)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(AMR_AF = amr)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(EAS_AF = eas)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(EUR_AF = eur)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(SAS_AF = sas)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(Trait = trait)
GeneLevel_SumTab <- GeneLevel_SumTab %>% rename(p_value = p)

GeneLevel_SumTab$Beta_SE <- paste(format(round(GeneLevel_SumTab$beta, 2), nsmall = 3), format(round(GeneLevel_SumTab$se, 3), nsmall = 2), sep = " (", collapse = NULL)
GeneLevel_SumTab$Beta_SE <- paste(GeneLevel_SumTab$Beta_SE, "", sep = ")", collapse = NULL)
GeneLevel_SumTab$source <- GeneLevel_SumTab$study
Phenoscanner_SumTab = subset(GeneLevel_SumTab, select = c("Query_gene", "var2", "Trait", "Beta_SE", "p_value", "Nearest_gene", "source", "rsid", "n"))

######  import data extracted from type 2 diabetes knowledge portal then merge with phenoscanner data
##
T2KP_summary <- read_excel("T2KP_summary_Sept21.xlsx")

## filter for relevant phenotypes
T2KP_summary = subset(T2KP_summary, phenotype =='allDKDadjHbA1cBMI' | phenotype =='ALP' | phenotype =='ALT' | phenotype =='BILIRUBIN' | phenotype =='BMI' | phenotype =='CAD' | phenotype =='CHOL' | phenotype =='CKD' | phenotype =='Creatinine' | phenotype =='DBP' | phenotype =='eGFRcrea' | phenotype =='HDL' | phenotype =='HDL2chol' | phenotype =='HDL3chol' | phenotype =='lateDKDadjHbA1cBMI' | phenotype =='PulsePress' | phenotype =='SBP' | phenotype =='T2D' | phenotype =='TG' | phenotype =='WHR' | phenotype =='WHRadjBMI')

# drop very rare variants (MAF <.01)
T2KP_summary = subset(T2KP_summary, maf >=.01)


T2KP_sum2 <- T2KP_summary %>% rename(Query_gene = Gene_search)
T2KP_sum2 <- T2KP_sum2 %>% rename(Nearest_gene = nearest)
T2KP_sum2 <- T2KP_sum2 %>% rename(p_value = pValue)
T2KP_sum2 <- T2KP_sum2 %>% rename(Trait = description)
T2KP_sum2$var2 <- paste(T2KP_sum2$dbSNP, T2KP_sum2$reference, sep = " ", collapse = NULL)
T2KP_sum2$var2 <- paste(T2KP_sum2$var2, T2KP_sum2$alt, sep = ">", collapse = NULL)
T2KP_sum2$Beta_SE <- paste(format(round(T2KP_sum2$beta, 2), nsmall = 3), format(round(T2KP_sum2$stdErr, 3), nsmall = 2), sep = " (", collapse = NULL)
T2KP_sum2$Beta_SE <- paste(T2KP_sum2$Beta_SE, "", sep = ")", collapse = NULL)
T2KP_sum2$source <- "CMDKP"
T2KP_sum2 <- T2KP_sum2 %>% rename(rsid = dbSNP)
T2KP_SumTab = subset(T2KP_sum2, select = c("Query_gene", "var2", "Trait", "Beta_SE", "p_value", "Nearest_gene", "source", "rsid", "n"))

## merge two gwas sumstat tables and filter


Phenoscanner_SumTab$n <- as.character(Phenoscanner_SumTab$n)
T2KP_SumTab$n <- as.character(T2KP_SumTab$n)

GeneLevel_merge1 <- rbind(Phenoscanner_SumTab, T2KP_SumTab)
GeneLevel_merge1 <- GeneLevel_merge1 %>% group_by(Query_gene, Trait) %>% arrange(p_value) %>% slice(1:1)

## get eQTLs for variants
Var_qtls <- get_qtls(GeneLevel_merge1$rsid, corr = 0.8, ld_method = "r2")

## filter for genes of interest
Var_qtls_filt = subset(Var_qtls, gene =="ACVR2A" | gene =="CIDEB" | gene =="FOXO1" | gene =="GPAM" | gene =="NEAT1" | gene =="TNRC6B" | gene =="ALB")
Var_qtls_filt <- Var_qtls_filt %>% group_by(query_term) %>% arrange(p) %>% distinct(tissue, .keep_all= TRUE)

Var_qtls_filt <- Var_qtls_filt %>% rename(rsid = query_term)

## filter by relevant tissues
Var_qtls_filt = subset(Var_qtls_filt, tissue =="Adipose - Subcutaneous" | tissue =="Adipose - Subcutaneous" |  tissue =="Adipose - Visceral (Omentum)" |  tissue =="Liver" | tissue =="Pancreas"| tissue =="Muscle skeletal" |  tissue =="Artery - Aorta"  |  tissue =="Artery - Coronary"  |  tissue =="Artery - Tibial"  |  tissue =="Atherosclerotic aortic root")

GeneLevel_merge2 <- merge(GeneLevel_merge1, Var_qtls_filt, by="rsid", all= TRUE)
write.table(GeneLevel_merge2,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/SupTab3.csv",sep=",")

## make filtered, formatted table for Table 3

GeneLevel_merge3 <- GeneLevel_merge2
GeneLevel_merge3$dir <- ifelse(GeneLevel_merge3$beta <=0, "(-)", "(+)")
GeneLevel_merge3$qtl_annot <- paste(GeneLevel_merge3$tissue, GeneLevel_merge3$dir, sep = " ", collapse = NULL)
GeneLevel_merge3 <- GeneLevel_merge3 %>% group_by(var2, Trait, gene) %>% mutate(eQTL= toString(qtl_annot))
GeneLevel_merge3 <- GeneLevel_merge3 %>% distinct(var2, Trait, gene, .keep_all=TRUE)
GeneLevel_merge3 = subset(GeneLevel_merge3, p_value < 5e-8)
GeneLevel_merge3$var3 <- paste(GeneLevel_merge3$proxy_variant, GeneLevel_merge3$nea, sep = " ", collapse = NULL)
GeneLevel_merge3$var3 <- paste(GeneLevel_merge3$var3, GeneLevel_merge3$ea, sep = ">", collapse = NULL)
GeneLevel_merge4 = subset(GeneLevel_merge3, select = c("Query_gene", "var2", "Trait", "Beta_SE", "p_value", "n", "Nearest_gene", "var3", "gene", "eQTL"))

GeneLevel_merge4 <- GeneLevel_merge4 %>% arrange(Query_gene, Trait, gene)
write.table(GeneLevel_merge4,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/Table3.csv",sep=",")

