#Genebass sig: sig <2.5 x 10-8 (STAT-KO)
#AZ PheWAS sig: suggestive neglogp 7, sig neglogp 8.7

#import AZ data

ACVR2A_binary <- read_csv("AZ_PheWAS_data/GeneLevel/ACVR2A_filtered_binary_traits_table_AZ_PheWAS_Portal.csv")
ACVR2A_binary$gene <- "ACVR2A"
ACVR2A_binary$analysis <- "GeneLevel"
ACVR2A_binary$source <- "AZ"
ACVR2A_binary$type <- "binary"
ACVR2A_binary <- ACVR2A_binary %>% rename(ColMod = "Collapsing model")
ACVR2A_binary <- ACVR2A_binary %>% rename(pval = "P value")
ACVR2A_binary <- ACVR2A_binary %>% rename(EffectSize = "Odds ratio")
ACVR2A_binary <- ACVR2A_binary %>% rename(LCI = "Odds ratio LCI")
ACVR2A_binary <- ACVR2A_binary %>% rename(UCI = "Odds ratio UCI")
ACVR2A_binary <- ACVR2A_binary %>% rename(CasesVar = "No. cases with QV")
ACVR2A_binary <- ACVR2A_binary %>% rename(CtrlVar = "No. controls with QV")
ACVR2A_binary <- ACVR2A_binary %>% rename(Total = "No. samples")
ACVR2A_binary$Sum_with_var <- ACVR2A_binary$CasesVar + ACVR2A_binary$CtrlVar
ACVR2A_binary = subset(ACVR2A_binary, select = c("gene", "Phenotype", "ColMod", "EffectSize", "LCI", "UCI", "pval", "Sum_with_var", "Total", "analysis", "type", "source"))
ACVR2A_continuous <- read_csv("AZ_PheWAS_data/GeneLevel/ACVR2A_filtered_continuous_traits_table_AZ_PheWAS_Portal.csv")
ACVR2A_continuous$gene <- "ACVR2A"
ACVR2A_continuous$analysis <- "GeneLevel"
ACVR2A_continuous$source <- "AZ"
ACVR2A_continuous$type <- "continuous"
ACVR2A_continuous <- ACVR2A_continuous %>% rename(ColMod = "Collapsing model")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(pval = "P value")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(EffectSize = "Effect size")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(LCI = "Effect size LCI")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(UCI = "Effect size UCI")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(Sum_with_var = "No. with QV")
ACVR2A_continuous <- ACVR2A_continuous %>% rename(Total = "No. samples")
ACVR2A_continuous = subset(ACVR2A_continuous, select = c("gene", "Phenotype", "ColMod", "EffectSize", "LCI", "UCI", "pval", "Sum_with_var", "Total", "analysis", "type", "source"))
ACVR2A_AZ <- rbind(ACVR2A_binary, ACVR2A_continuous)

