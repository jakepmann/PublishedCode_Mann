ALT_metan_ACVR2A <- ALT_metan_ACVR2A %>% rename(alt = effect_allele)
ALT_metan_ACVR2A <- ALT_metan_ACVR2A %>% rename(EAF = effect_allele_frequency)
ALT_metan_ACVR2A = subset(ALT_metan_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

