## PNPLA3
LiverDis_PNPLA3 <- read.table(file = 'LiverDis_UKBB_PNPLA3.tsv', sep = '\t', header = FALSE)
colnames(LiverDis_PNPLA3) <- LiverDis_header
LiverDis_PNPLA3 $trait <- "LiverDis"
LiverDis_PNPLA3$HCCgene <- "PNPLA3"
LiverDis_PNPLA3 <- LiverDis_PNPLA3 %>% rename(beta = beta_EUR)
LiverDis_PNPLA3 <- LiverDis_PNPLA3 %>% rename(se = se_EUR)
LiverDis_PNPLA3 <- LiverDis_PNPLA3 %>% rename(pval = pval_EUR)
LiverDis_PNPLA3 <- LiverDis_PNPLA3 %>% rename(EAF = af_controls_EUR)
LiverDis_PNPLA3 = subset(LiverDis_PNPLA3, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

ALT_PNPLA3 <- read.table(file = 'ALT_UKBB_PNPLA3.tsv', sep = '\t', header = FALSE)
colnames(ALT_PNPLA3) <- ALT_header
ALT_PNPLA3$trait <- "ALT"
ALT_PNPLA3$HCCgene <- "PNPLA3"
ALT_PNPLA3 = subset(ALT_PNPLA3, beta_EUR !="")
ALT_PNPLA3 <- ALT_PNPLA3 %>% rename(beta = beta_EUR)
ALT_PNPLA3 <- ALT_PNPLA3 %>% rename(se = se_EUR)
ALT_PNPLA3 <- ALT_PNPLA3 %>% rename(pval = pval_EUR)
ALT_PNPLA3 <- ALT_PNPLA3 %>% rename(EAF = af_EUR)
ALT_PNPLA3 = subset(ALT_PNPLA3, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

