ACVR2A_synon_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000121989_ACVR2A_synon.csv")

CIDEB_pLoF_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000136305_CIDEB_pLoF.csv")
CIDEB_mis_genebass <- read_csv("Genebass_data/gene-phewas-exomes_ENSG00000136305_CIDEB_mis.csv")
