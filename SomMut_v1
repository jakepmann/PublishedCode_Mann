BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(se = SE)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(pval = P)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(chr = CHR)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(pos = POS)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(ref = REF)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(alt = ALT)
BBJ_ALT_PNPLA3 <- BBJ_ALT_PNPLA3 %>% rename(EAF = Frq)
BBJ_ALT_PNPLA3 = subset(BBJ_ALT_PNPLA3, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

BBJ_HbA1c_PNPLA3 <- read.table(file = 'BBJ_HbA1c_PNPLA3.tsv', sep = '\t', header = FALSE)
colnames(BBJ_HbA1c_PNPLA3) <- BBJ_HbA1c_header
BBJ_HbA1c_PNPLA3$trait <- "BBJ_HbA1c"
BBJ_HbA1c_PNPLA3$HCCgene <- "PNPLA3"
BBJ_HbA1c_PNPLA3 = subset(BBJ_HbA1c_PNPLA3, beta !="")
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(beta = BETA)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(se = SE)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(pval = P)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(chr = CHR)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(pos = POS)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(ref = REF)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(alt = ALT)
BBJ_HbA1c_PNPLA3 <- BBJ_HbA1c_PNPLA3 %>% rename(EAF = Frq)
BBJ_HbA1c_PNPLA3 = subset(BBJ_HbA1c_PNPLA3, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

PNPLA3_summary <- rbind(LiverDis_PNPLA3, ALD_PNPLA3, Cirrhosis_PNPLA3, ALT_PNPLA3, AST_PNPLA3, Fibrosis_PNPLA3, NAFLD_PNPLA3, HbA1c_PNPLA3, FinnGen_HCC_PNPLA3, FinnGen_Cirrhosis_PNPLA3, FinnGen_NAFLD_PNPLA3, FinnGen_ALCOLIVER_PNPLA3, Liu_LiverFat_PNPLA3, Anstee_PNPLA3)
PNPLA3_rep_summary <- rbind(MAGIC_HbA1c_PNPLA3, Sprack_T2DM_PNPLA3, ALT_metan_PNPLA3, BBJ_ALT_PNPLA3, BBJ_HbA1c_PNPLA3)
PNPLA3_overall_summary <- rbind(PNPLA3_summary, PNPLA3_rep_summary)

