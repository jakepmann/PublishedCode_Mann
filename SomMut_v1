BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(beta = BETA)
BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(se = SE)
BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(pval = P)
BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(chr = CHR)
BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(pos = POS)
BBJ_HbA1c_HSD17B13 <- BBJ_HbA1c_HSD17B13 %>% rename(ref = REF)
