Anstee_GPAM <- read.table(file = 'Anstee_GPAM.tsv', sep = '\t', header = FALSE)
colnames(Anstee_GPAM) <- Anstee_header
Anstee_GPAM$trait <- "Anstee_NAFLD_Dx"
Anstee_GPAM$HCCgene <- "GPAM"
Anstee_GPAM = subset(Anstee_GPAM, odds_ratio !="")
Anstee_GPAM <- Anstee_GPAM %>% filter(between(base_pair_location, 113909624, 113975135))
Anstee_GPAM <- Anstee_GPAM %>% rename(beta = odds_ratio)
Anstee_GPAM <- Anstee_GPAM %>% rename(se = standard_error)
Anstee_GPAM <- Anstee_GPAM %>% rename(pval = p_value)
Anstee_GPAM <- Anstee_GPAM %>% rename(chr = chromosome)
Anstee_GPAM <- Anstee_GPAM %>% rename(pos = base_pair_location)
Anstee_GPAM <- Anstee_GPAM %>% rename(ref = other_allele)
Anstee_GPAM <- Anstee_GPAM %>% rename(alt = effect_allele)
Anstee_GPAM$EAF <- ""
Anstee_GPAM = subset(Anstee_GPAM, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

Anstee_NEAT1 <- read.table(file = 'Anstee_NEAT1.tsv', sep = '\t', header = FALSE)
colnames(Anstee_NEAT1) <- Anstee_header
Anstee_NEAT1$trait <- "Anstee_NAFLD_Dx"
Anstee_NEAT1$HCCgene <- "NEAT1"
