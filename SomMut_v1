./tabtk/tabtk grep -f 2 "14" /Users/jakemann/UKBB_sumstats/BBJ.HbA1c.autosome.txt > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr14.tsv
./tabtk/tabtk grep -f 2 "13" /Users/jakemann/UKBB_sumstats/BBJ.HbA1c.autosome.txt > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr13.tsv
./tabtk/tabtk grep -f 2 "10" /Users/jakemann/UKBB_sumstats/BBJ.HbA1c.autosome.txt > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr10.tsv
./tabtk/tabtk grep -f 2 "11" /Users/jakemann/UKBB_sumstats/BBJ.HbA1c.autosome.txt > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr11.tsv
./tabtk/tabtk grep -f 2 "22" /Users/jakemann/UKBB_sumstats/BBJ.HbA1c.autosome.txt > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr22.tsv
./tabtk/tabtk grep -f 3 "1486[0-8][0-9][0-9][0-9][0-9]" /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_chr2.tsv > /Users/jakemann/UKBB_sumstats/BBJ_HbA1c_ACVR2A.tsv
