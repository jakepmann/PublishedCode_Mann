BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(beta = BETA)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(se = SE)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(pval = P)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(chr = CHR)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(pos = POS)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(ref = REF)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(alt = ALT)
BBJ_ALT_FOXO1 <- BBJ_ALT_FOXO1 %>% rename(EAF = Frq)
BBJ_ALT_FOXO1 = subset(BBJ_ALT_FOXO1, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

