NAFLD_HSD17B13 <- NAFLD_HSD17B13 %>% rename(beta = beta_EUR)
NAFLD_HSD17B13 <- NAFLD_HSD17B13 %>% rename(se = se_EUR)
NAFLD_HSD17B13 <- NAFLD_HSD17B13 %>% rename(pval = pval_EUR)
NAFLD_HSD17B13 <- NAFLD_HSD17B13 %>% rename(EAF = af_controls_EUR)
NAFLD_HSD17B13 = subset(NAFLD_HSD17B13, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

