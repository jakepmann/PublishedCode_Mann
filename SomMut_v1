MAGIC_HbA1c_TM6SF2$EAF <- ""
MAGIC_HbA1c_TM6SF2 = subset(MAGIC_HbA1c_TM6SF2, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

