#summary table of replication data

ACVR2A_rep_summary <- rbind(MAGIC_HbA1c_ACVR2A, Sprack_T2DM_ACVR2A, ALT_metan_ACVR2A, BBJ_ALT_ACVR2A, BBJ_HbA1c_ACVR2A)
ALB_rep_summary <- rbind(MAGIC_HbA1c_ALB, Sprack_T2DM_ALB, ALT_metan_ALB, BBJ_ALT_ALB, BBJ_HbA1c_ALB)
CIDEB_rep_summary <- rbind(MAGIC_HbA1c_CIDEB, Sprack_T2DM_CIDEB, ALT_metan_CIDEB, BBJ_ALT_CIDEB, BBJ_HbA1c_CIDEB)
FOXO1_rep_summary <- rbind(MAGIC_HbA1c_FOXO1, Sprack_T2DM_FOXO1, ALT_metan_FOXO1, BBJ_ALT_FOXO1, BBJ_HbA1c_FOXO1)
GPAM_rep_summary <- rbind(MAGIC_HbA1c_GPAM, Sprack_T2DM_GPAM, ALT_metan_GPAM, BBJ_ALT_GPAM, BBJ_HbA1c_GPAM)
NEAT1_rep_summary <- rbind(MAGIC_HbA1c_NEAT1, Sprack_T2DM_NEAT1, ALT_metan_NEAT1, BBJ_ALT_NEAT1, BBJ_HbA1c_NEAT1)
TNRC6B_rep_summary <- rbind(MAGIC_HbA1c_TNRC6B, Sprack_T2DM_TNRC6B, ALT_metan_TNRC6B, BBJ_ALT_TNRC6B, BBJ_HbA1c_TNRC6B)

GWAS_rep_sumtab <- rbind(ACVR2A_rep_summary, ALB_rep_summary, CIDEB_rep_summary, FOXO1_rep_summary, GPAM_rep_summary, NEAT1_rep_summary, TNRC6B_rep_summary)

## albumin data has included some positions from chr14! so will need to remove those
GWAS_rep_sumtab_drop = subset(GWAS_rep_sumtab, HCCgene=="ALB" & chr=="14")
GWAS_rep_sumtab <- anti_join(GWAS_rep_sumtab, GWAS_rep_sumtab_drop, by=c("HCCgene", "chr", "pos"))


## make figures from replication data

GWAS_rep_sumtab <- GWAS_rep_sumtab %>% group_by(trait) %>% arrange(chr, pos) %>% mutate(bp = row_number())

MAGIC_HbA1c_fig <- ggplot(subset(GWAS_rep_sumtab, trait=="MAGIC_HbA1c"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "HbA1c (MAGIC: n=204,339)")
pdf(file="Fig_MAGIC_HbA1c.pdf",width=10,height=3)
MAGIC_HbA1c_fig
dev.off()

Sprack_T2DM_fig <- ggplot(subset(GWAS_rep_sumtab, trait=="Sprack_T2DM"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "Type 2 diabetes diagnosis (East Asian GWAS meta-analysis: n=433,540)")
pdf(file="Fig_Sprack_T2DM.pdf",width=10,height=3)
Sprack_T2DM_fig
dev.off()

ALT_metan_fig <- ggplot(subset(GWAS_rep_sumtab, trait=="ALT_metan"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "ALT (European GWAS meta-analysis: n=753,010)")
pdf(file="Fig_ALT_metan.pdf",width=10,height=3)
ALT_metan_fig
dev.off()

BBJ_ALT_fig <- ggplot(subset(GWAS_rep_sumtab, trait=="BBJ_ALT"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "ALT (BioBank Japan: n=134,182)")
pdf(file="Fig_BBJ_ALT.pdf",width=10,height=3)
BBJ_ALT_fig
dev.off()

BBJ_HbA1c_fig <- ggplot(subset(GWAS_rep_sumtab, trait=="BBJ_HbA1c"), aes(x=bp, y=-log10(pval))) + theme_bw() + geom_point(aes(color=as.factor(chr)), alpha=0.8, size=1.3) + facet_wrap(~ HCCgene, scales = "free_x", nrow=1, strip.position="bottom") + geom_hline(yintercept = -log10(5e-8), size=0.5, linetype="dashed")  + scale_y_continuous(limits=c(0, 15), breaks=seq(0, 15, 5), expand = c(0,0)) + scale_color_manual(values = cbPalette) + theme(legend.position = "none", axis.title.x=element_blank(), axis.text.x=element_blank(), panel.spacing.x = unit(0.05, "lines"), panel.border = element_rect(size=0.1), panel.grid.minor = element_blank(), panel.grid.major = element_blank(), axis.ticks.x = element_blank(), strip.text.x = element_text(face = "italic")) + labs(title = "HbA1c (BioBank Japan: n=42,790)")
pdf(file="Fig_BBJ_HbA1c.pdf",width=10,height=3)
BBJ_HbA1c_fig
dev.off()

## write table including all summary stats from figures
GWAS_sumstat_full <- rbind(GWAS_sumstat_sumtab, GWAS_rep_sumtab)
GWAS_sumstat_full = subset(GWAS_sumstat_full, select = -c(bp))
write.table(GWAS_sumstat_full,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/SupTab2.csv",sep=",")

## filter GWAS sumstats for significant associations and then annotate with liver eQTLs

GWAS_sig = subset(GWAS_sumstat_full, pval <= 5e-8)
GWAS_sig <- GWAS_sig %>% group_by(HCCgene, trait) %>% arrange(pval) %>% slice(1:1)
GWAS_sig$qtl_input <- paste("chr",GWAS_sig$chr, GWAS_sig$pos, sep = ":", collapse = NULL)
write.table(GWAS_sig,file="/Users/jakemann/Documents/HCC_MattH/GWAS_sig.csv",sep=",")

## use FIVEx web tool to get eQTLs & sQTLs for top variants

## make summary figures of plots

pdf(file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/Figure2.pdf",width=10,height=8)
plot_grid(ALT_fig, Liu_LiverFat_fig, HbA1c_fig, ALD_fig, ncol = 1, rel_heights = c(1.0, 1.0))
dev.off()

pdf(file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/SupFig1.pdf",width=10,height=10)
plot_grid(ALT_metan_fig, BBJ_ALT_fig, AST_fig, Anstee_NAFLD_Dx_fig, FinnGen_ALCOLIVER_fig, ncol = 1, rel_heights = c(1.0, 1.0))
dev.off()

