LiverDis_CIDEB <- LiverDis_CIDEB %>% rename(pval = pval_EUR)
LiverDis_CIDEB <- LiverDis_CIDEB %>% rename(EAF = af_controls_EUR)
LiverDis_CIDEB = subset(LiverDis_CIDEB, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

LiverDis_FOXO1 <- read.table(file = 'LiverDis_UKBB_FOXO1.tsv', sep = '\t', header = FALSE)
colnames(LiverDis_FOXO1) <- LiverDis_header
LiverDis_FOXO1$trait <- "LiverDis"
LiverDis_FOXO1$HCCgene <- "FOXO1"
LiverDis_FOXO1 = subset(LiverDis_FOXO1, beta_EUR !="")
