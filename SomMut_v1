GPAM_continuous$type <- "continuous"
GPAM_continuous <- GPAM_continuous %>% rename(ColMod = "Collapsing model")
GPAM_continuous <- GPAM_continuous %>% rename(pval = "P value")
GPAM_continuous <- GPAM_continuous %>% rename(EffectSize = "Effect size")
GPAM_continuous <- GPAM_continuous %>% rename(LCI = "Effect size LCI")
GPAM_continuous <- GPAM_continuous %>% rename(UCI = "Effect size UCI")
GPAM_continuous <- GPAM_continuous %>% rename(Sum_with_var = "No. with QV")
GPAM_continuous <- GPAM_continuous %>% rename(Total = "No. samples")
GPAM_continuous = subset(GPAM_continuous, select = c("gene", "Phenotype", "ColMod", "EffectSize", "LCI", "UCI", "pval", "Sum_with_var", "Total", "analysis", "type", "source"))
GPAM_AZ <- rbind(GPAM_binary, GPAM_continuous)

TNRC6B_binary <- read_csv("AZ_PheWAS_data/GeneLevel/TNRC6B_filtered_binary_traits_table_AZ_PheWAS_Portal.csv")
TNRC6B_binary$gene <- "TNRC6B"
TNRC6B_binary$analysis <- "GeneLevel"
TNRC6B_binary$source <- "AZ"
TNRC6B_binary$type <- "binary"
TNRC6B_binary <- TNRC6B_binary %>% rename(ColMod = "Collapsing model")
TNRC6B_binary <- TNRC6B_binary %>% rename(pval = "P value")
TNRC6B_binary <- TNRC6B_binary %>% rename(EffectSize = "Odds ratio")
TNRC6B_binary <- TNRC6B_binary %>% rename(LCI = "Odds ratio LCI")
TNRC6B_binary <- TNRC6B_binary %>% rename(UCI = "Odds ratio UCI")
TNRC6B_binary <- TNRC6B_binary %>% rename(CasesVar = "No. cases with QV")
TNRC6B_binary <- TNRC6B_binary %>% rename(CtrlVar = "No. controls with QV")
TNRC6B_binary <- TNRC6B_binary %>% rename(Total = "No. samples")
TNRC6B_binary$Sum_with_var <- TNRC6B_binary$CasesVar + TNRC6B_binary$CtrlVar
TNRC6B_binary = subset(TNRC6B_binary, select = c("gene", "Phenotype", "ColMod", "EffectSize", "LCI", "UCI", "pval", "Sum_with_var", "Total", "analysis", "type", "source"))
TNRC6B_continuous <- read_csv("AZ_PheWAS_data/GeneLevel/TNRC6B_filtered_continuous_traits_table_AZ_PheWAS_Portal.csv")
TNRC6B_continuous$gene <- "TNRC6B"
TNRC6B_continuous$analysis <- "GeneLevel"
TNRC6B_continuous$source <- "AZ"
TNRC6B_continuous$type <- "continuous"
TNRC6B_continuous <- TNRC6B_continuous %>% rename(ColMod = "Collapsing model")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(pval = "P value")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(EffectSize = "Effect size")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(LCI = "Effect size LCI")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(UCI = "Effect size UCI")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(Sum_with_var = "No. with QV")
TNRC6B_continuous <- TNRC6B_continuous %>% rename(Total = "No. samples")
TNRC6B_continuous = subset(TNRC6B_continuous, select = c("gene", "Phenotype", "ColMod", "EffectSize", "LCI", "UCI", "pval", "Sum_with_var", "Total", "analysis", "type", "source"))
TNRC6B_AZ <- rbind(TNRC6B_binary, TNRC6B_continuous)

# bind all together and filter as needed
AZ_gene_sum <- rbind(CIDEB_AZ , ALB_AZ, FOXO1_AZ , GPAM_AZ , TNRC6B_AZ, ACVR2A_AZ)
AZ_gene_sum$neglogp <- -log10(AZ_gene_sum$pval)
AZ_gene_sum = subset(AZ_gene_sum, neglogp >=7)
AZ_gene_sum <- AZ_gene_sum %>% group_by(gene, Phenotype) %>% arrange(pval) %>% slice(1:1)
write.table(AZ_gene_sum,file="/Users/jakemann/Documents/HCC_MattH/00_SomMut_JHepRep_v1/AZ_gene_sum.csv",sep=",")
## repeat for variant level results for AZ data

ACVR2A_var_binary <- read_csv("AZ_PheWAS_data/VarLevel/ACVR2A_filtered_binary_traits_table_AZ_PheWAS_Portal.csv")
ACVR2A_var_binary$gene <- "ACVR2A"
ACVR2A_var_binary$analysis <- "VarLevel"
ACVR2A_var_binary$source <- "AZ"
ACVR2A_var_binary$type <- "binary"
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(AA_change = "Amino acid change")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(ConsType = "Consequence type")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(pval = "p-value")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(VarType = "Variant type")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(EffectSize = "Odds ratio")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(LCI = "Odds ratio LCI")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(UCI = "Odds ratio UCI")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(CaseMAF = "Case MAF")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(CtrlMAF = "Control MAF")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(Ctrls = "No. controls")
ACVR2A_var_binary <- ACVR2A_var_binary %>% rename(Cases = "No. cases")
ACVR2A_var_binary$Total <- ACVR2A_var_binary$Cases + ACVR2A_var_binary$Ctrls
ACVR2A_var_binary$MAF <- ((ACVR2A_var_binary$Cases * ACVR2A_var_binary$CaseMAF) + (ACVR2A_var_binary$Ctrls * ACVR2A_var_binary$CtrlMAF)) / ACVR2A_var_binary$Total 
ACVR2A_var_binary = subset(ACVR2A_var_binary, select = c("gene", "Phenotype", "Variant", "AA_change", "VarType", "ConsType", "Model", "EffectSize", "LCI", "UCI", "pval", "MAF", "Total", "analysis", "type", "source"))
