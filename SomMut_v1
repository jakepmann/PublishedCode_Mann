MAGIC_HbA1c_HSD17B13 <- read.table(file = 'MAGIC_HbA1c_HSD17B13.tsv', sep = '\t', header = FALSE)
colnames(MAGIC_HbA1c_HSD17B13) <- MAGIC_HbA1c_header
MAGIC_HbA1c_HSD17B13$trait <- "MAGIC_HbA1c"
MAGIC_HbA1c_HSD17B13$HCCgene <- "HSD17B13"
MAGIC_HbA1c_HSD17B13 = subset(MAGIC_HbA1c_HSD17B13, log10BF !="")
MAGIC_HbA1c_HSD17B13$beta <- 10^MAGIC_HbA1c_HSD17B13$log10BF
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(se = standard_error)
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(pval = het_p_value)
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(chr = chromosome)
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(pos = base_pair_location)
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(ref = other_allele)
MAGIC_HbA1c_HSD17B13 <- MAGIC_HbA1c_HSD17B13 %>% rename(alt = effect_allele)
MAGIC_HbA1c_HSD17B13$EAF <- ""
MAGIC_HbA1c_HSD17B13 = subset(MAGIC_HbA1c_HSD17B13, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

