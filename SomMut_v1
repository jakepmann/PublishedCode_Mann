Sprack_T2DM_ACVR2A = subset(Sprack_T2DM_ACVR2A, beta !="")
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% filter(between(Pos, 148602086, 148688393))
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(beta = Beta)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(se = standard_error)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(pval = P)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(chr = Chr)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(pos = Pos)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(ref = NEA)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(alt = EA)
Sprack_T2DM_ACVR2A <- Sprack_T2DM_ACVR2A %>% rename(EAF = EAF)
Sprack_T2DM_ACVR2A = subset(Sprack_T2DM_ACVR2A, select = c("chr", "pos", "ref", "alt", "HCCgene", "trait", "beta", "pval", "EAF"))

